test:ta111 a skos:Concept ;
    skos:broader test:ta1 ;
    skos:inScheme test:conceptscheme ;
    skos:prefLabel "Matematiikka"@fi ;
    skos:scopeNote "Matematiikka, soveltava matematiikka ja todennäköisyyslaskenta"@fi .

test:ta112 a skos:Concept ;
