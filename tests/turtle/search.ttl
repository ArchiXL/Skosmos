    skos:inScheme test:conceptscheme ;
    skos:prefLabel "Tilastotiede"@fi ;
    skos:scopeNote "Sisältää tilastollisten menetelmien tutkimuksen, mutta ei soveltavan tilastotieteen tutkimusta, joka luokitellaan kuuluvaksi kulloiseenkin sovellusalueeseen (taloustiede, sosiologia jne.)"@fi .

test:ta113 a skos:Concept ;
    skos:broader test:ta1 ;
    skos:inScheme test:conceptscheme ;
    skos:prefLabel "Tietojenkäsittely ja informaatiotieteet"@fi .

test:ta114 a skos:Concept ;
    skos:broader test:ta1 ;
    skos:inScheme test:conceptscheme ;
    skos:prefLabel "Fysiikka"@fi .

test:ta115 a skos:Concept ;
    skos:broader test:ta1 ;
    skos:inScheme test:conceptscheme ;
