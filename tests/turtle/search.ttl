        test:ta112,
        test:ta113,
        test:ta114,
        test:ta115,
        test:ta116,
        test:ta1171,
        test:ta1172,
        test:ta1181,
        test:ta1182,
        test:ta1183,
        test:ta1184,
        test:ta119 ;
    skos:prefLabel "LUONNONTIETEET"@fi ;
    skos:topConceptOf test:conceptscheme .

test:conceptscheme a skos:ConceptScheme ;
    rdfs:label "Test conceptscheme"en ;
    skos:hasTopConcept test:ta1 .

