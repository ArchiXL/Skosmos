clear

echo 'writing translations: ...'
msgfmt onkiLight_en.po
mv messages.mo en/LC_MESSAGES/onkiLight.mo

msgfmt onkiLight_fi.po
mv messages.mo fi/LC_MESSAGES/onkiLight.mo

msgfmt onkiLight_sv.po
mv messages.mo sv/LC_MESSAGES/onkiLight.mo
echo 'translations ready ...'

sudo service apache2 restart
